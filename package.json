{
  "name": "leointeg",
  "displayName": "leointeg",
  "description": "Leo Editor Integration with VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openLeoFile",
    "onView:leoIntegration"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openLeoFile",
        "title": "Open Leo File"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "leo-integration",
          "title": "Leo Integration",
          "icon": "resources/leo-integration-icon.svg"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "leoIntegration",
          "name": "Outline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.2"
  }
}
