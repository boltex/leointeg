    );
    this.refreshGotoPane = debounce(
        () =&gt; { this._leoGotoProvider.refreshTreeRoot(); },
        Constants.GOTO_DEBOUNCE_DELAY
    );
    this.refreshAll = debounce(
        () =&gt; {
            this.launchRefresh({
                tree: true,
                body: true,
                buttons: true,
                states: true,
                documents: true
            }, false);
        },
        Constants.REFRESH_ALL_DEBOUNCE_DELAY
    );
}

