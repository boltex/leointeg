export function buildNodeCommandJson(p_nodeJson: string, p_command?: UserCommand): string {
    let w_json = "{\"ap\":" + p_nodeJson; // already json
    if (p_command &amp;&amp; p_command.name) {
        w_json += ", \"name\": " + JSON.stringify(p_command.name);
