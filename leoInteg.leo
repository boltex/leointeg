<t tx="felix.20200924211054.1">def _outputPNodes(self, p_pList):
    w_apList = []
    for p in p_pList:
        w_apList.append(self._p_to_ap(p))
    # Multiple nodes, plural
    return self.sendLeoBridgePackage("nodes", w_apList)

