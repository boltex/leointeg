<t tx="felix.20200924211051.1">def _outputPNode(self, p_node=False):
    if p_node:
        # Single node, singular
        return self.sendLeoBridgePackage("node", self._p_to_ap(p_node))
    else:
        return self.sendLeoBridgePackage("node", None)

