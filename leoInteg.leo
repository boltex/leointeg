                        return this.sendAction(
                            Constants.LEOBRIDGE.CLICK_BUTTON,
                            JSON.stringify({ index: p_node.button.index, rclick: this._rclickSelected })
                        );
                    }
                    // Escaped
                    return Promise.reject();
                });


            } else {
                // Normal button
                return this.sendAction(
                    Constants.LEOBRIDGE.CLICK_BUTTON,
                    JSON.stringify({ index: p_node.button.index })
                );
            }
