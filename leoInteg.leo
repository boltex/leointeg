<t tx="fil.20210603223933.3">function adjustLight(color: number, amount: number) {
    const cc = color + amount;
    const c = amount &lt; 0 ? (cc &lt; 0 ? 0 : cc) : cc &gt; 255 ? 255 : cc;

    return Math.round(c);
}

