                throw vscode.FileSystemError.FileNotFound();
            } else {
                return this._leoIntegration.sendAction(Constants.LEOBRIDGE.GET_BODY_LENGTH, '"' + w_gnx + '"')
                    .then((p_result) =&gt; {
                        let w_mtime: number = 0;
                        if (this._renameBody.gnx === w_gnx) {
